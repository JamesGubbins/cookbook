      <recipeFrame id="1">
         <title>Japanese Curry</title>
         <IngredientFrames class="java.util.ArrayList">
            <ingredientFrame name="S and B Golden Curry Mix" quantity="1" unit="Pack"/>
            <ingredientFrame name="Chicken, Lamb, Pork, Beef" quantity="500" unit="Grams"/>
            <ingredientFrame name="Carrot" quantity="1" unit=""/>
            <ingredientFrame name="Brown Onion" quantity="1" unit=""/>
            <ingredientFrame name="Potato" quantity="1" unit=""/>
            <ingredientFrame name="Long Grain Rice" quantity="1" unit="Cup"/>
            <ingredientFrame name="Vegetable Oil" quantity="1" unit="Tablespoon"/>
         </IngredientFrames>
         <time>30</time>
         <serving>5</serving>
         <stepFrames class="java.util.ArrayList">
            <stepFrame no="1" desc="Start cooking rice in rice cooker"/>
            <stepFrame no="2" desc="Dice the meat and chop the vegetables"/>
            <stepFrame no="3" desc="Stir-fry the meat and vegetables in a large pot"/>
            <stepFrame no="4" desc="Add 540ml of water to the pot and bring to boil"/>
            <stepFrame no="5" desc="Take off heat and add the curry mix to the pot"/>
